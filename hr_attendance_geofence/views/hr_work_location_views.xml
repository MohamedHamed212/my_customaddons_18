<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_hr_work_location_form_geofence" model="ir.ui.view">
        <field name="name">hr.work.location.form.geofence</field>
        <field name="model">hr.work.location</field>
        <field name="inherit_id" ref="hr.hr_work_location_form_view"/>
        <field name="arch" type="xml">
            <field name="address_id" position="after">
                <field name="longitude"/>
                <field name="latitude"/>
                <field name="range"/>
            </field>
        </field>
    </record>
</odoo>
